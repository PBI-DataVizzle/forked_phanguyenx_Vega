      "source": "binned_group",
      "transform": [
        {
          "type": "formula",
          "expr": "375",
          "as": "bar_y"
        },
        {
          "type": "formula",
          "expr": "scale('xscale', datum.WADPD_group) + (bandwidth('xscale')/4)",
          "as": "bar_x_mid"
        }
      ]
    },
    {
      "name": "trees-location",
      "source": "merged",
      "transform": [
        {
          "type": "geopoint",
          "projection": "projection",
          "fields": [
            "__3__",
            "__2__"
          ],
