    {
      "name": "aggregated_binned",
      "source": "binned_group",
      "transform": [
        {
          "type": "aggregate",
          "groupby": [
            "WADPD_group"
          ],
          "ops": [
            "count",
            "sum"
          ],
          "fields": [
            "__0__",
            "__4__"
          ],
          "as": [
            "count",
            "Net Amount"
          ]
        },
        {
          "type": "formula",
          "as": "AxisXLabels",
          "expr": "datum.WADPD_group == 'Current' ? datum.WADPD_group : join(slice(split(datum.WADPD_group, ' '), 0, 1), '\\n') + '\\n' + join(slice(split(datum.WADPD_group, ' '), 1, 2), ' ') + ' ' + join(slice(split(datum.WADPD_group, ' '), 2), '\\n')"
        }
      ]
    },
    {
      "name": "merged",
      "source": "trees",
      "transform": [
        {
          "type": "lookup",
          "from": "binned_group",
