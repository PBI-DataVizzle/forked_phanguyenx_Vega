            "proj_x",
            "proj_y"
          ]
        }
      ]
    },
    {
      "name": "edges",
      "source": "merged",
      "transform": [
        {
          "type": "formula",
          "expr": "datum.WADPD_group",
          "as": "s"
        },
        {
          "type": "formula",
          "expr": "datum['__0__']",
          "as": "t"
        },
        {
          "type": "lookup",
          "from": "bar-positions",
          "key": "WADPD_group",
          "fields": [
            "s"
          ],
          "as": [
            "source"
          ]
        },
        {
          "type": "lookup",
          "from": "trees-location",
          "key": "__0__",
          "fields": [
            "t"
          ],
          "as": [
            "target"
          ]
        },
        {
          "type": "linkpath",
          "sourceX": "source.bar_x_mid",
          "sourceY": "source.bar_y",
          "targetX": "target.proj_x",
          "targetY": "target.proj_y",
          "orient": "vertical",
          "shape": "diagonal"
        }
      ]
    }
